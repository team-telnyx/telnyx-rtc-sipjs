{
  "name": "@telnyx/rtc-sipjs-simple-user",
  "version": "0.0.1",
  "description": "Telnyx JavaScript library for building WebRTC apps on top of SIP.js SimpleUser",
  "keywords": [
    "telnyx",
    "webrtc",
    "sip",
    "sip.js",
    "soft phone",
    "voip"
  ],
  "main": "dist/telnyx-rtc-sipjs.js",
  "types": "dist/types/telnyx-rtc-sipjs.d.ts",
  "files": [
    "dist/",
    "types/",
    "docs/"
  ],
  "scripts": {
    "docs": "jsdoc2md src/lib/telnyx-device.ts > docs/TelnyxDevice.md && jsdoc2md src/lib/telnyx-call.ts > docs/TelnyxCall.md",
    "clean": "rimraf dist/*",
    "prebuild": "yarn clean",
    "build": "yarn build:bundles && yarn types",
    "build:bundles": "yarn build:dev && yarn build:prod",
    "build:dev": "webpack --config ./webpack.config.dev.js",
    "build:prod": "webpack --config ./webpack.config.prod.js",
    "watch": "webpack --watch --config ./webpack.config.dev.js",
    "start": "yarn watch",
    "test": "karma start",
    "types": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepare": "yarn build"
  },
  "contributors": [
    {
      "name": "Jason Brackins",
      "email": "jasonb@telnyx.com",
      "url": "https://telnyx.com"
    },
    {
      "name": "Sua Yoo",
      "email": "sua@telnyx.com",
      "url": "https://telnyx.com"
    }
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-telnyx/telnyx-rtc-sipjs"
  },
  "publishConfig": {
    "@telnyx:registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "es6-event-emitter": "^1.5.2",
    "jasmine-core": "^5.2.0",
    "jsdoc-to-markdown": "^8.0.0",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "rimraf": "^5.0.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "sip.js": "0.21.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.1.1"
}
